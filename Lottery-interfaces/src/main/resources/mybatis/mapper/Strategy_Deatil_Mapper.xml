<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.l13z.lottery.infrastructure.dao.IStrategyDetailDao">

    <resultMap id="StrategyDeatilMap" type="cn.l13z.lottery.infrastructure.po.StrategyDetail">
        <id column="id" property="id"/>
        <result column="strategyId" property="strategyId"/>
        <result column="awardId" property="awardId"/>
        <result column="awardCount" property="awardCount"/>
        <result column="awardRate" property="awardRate"/>
        <result column="createTime" property="createTime"/>
        <result column="updateTime" property="updateTime"/>

    </resultMap>

    <select id="queryStrategyDetailList" parameterType="java.lang.Long" resultType="cn.l13z.lottery.infrastructure.po.StrategyDetail">
        SELECT id, strategyId, awardId, awardCount, awardRate, createTime, updateTime
        FROM strategy_detail
        WHERE strategyId = #{strategyId}
    </select>
</mapper>
